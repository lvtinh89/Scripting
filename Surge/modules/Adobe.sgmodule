[MITM]
hostname = %APPEND% lcs-mobile-cops.adobe.io, createpdf.acrobat.com
[Script]
Adobe Photoshop = type=http-response,pattern=https://lcs-mobile-cops.adobe.io/mobile_profile/nul/v2,requires-body=1,max-size=0,control-api=1,script-path=https://raw.githubusercontent.com/lvtinh89/Scripting/master/Adobe_edit.js
